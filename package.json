{
  "name": "tsp-npm-registry",
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "@typespec/compiler": "latest",
    "@typespec/rest": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/versioning": "latest",
    "@azure-tools/typespec-azure-core": "latest",
    "@azure-tools/typespec-autorest": "latest",
    "tsp-rust": "file:./tsp-rust"
  },
  "scripts": {
    "build": "tsp compile ./main.tsp && npm run build:openapi3",
    "build:openapi3": "openapi-generator-cli generate -i tsp-output/@typespec/openapi3/openapi.yaml -o ./client/ -g rust --additional-properties=packageName=npm_registry"
  },
  "private": true
}